{
    "name": "ahmyi/cakephp-datatables",
    "description": "DataTables plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5",
        "php":"^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Datatables\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datatables\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
